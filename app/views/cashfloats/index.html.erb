<p id="notice"><%= notice %></p>

<h2>Listing Cashfloats</h2>

<table class="table">
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cashfloats.each do |cashfloat| %>
      <tr>
        <td><%= cashfloat.created_at %></td>
        <td><%= cashfloat.float_type %></td>
        <td><%= cashfloat.period %></td>
        <td><%= cashfloat.completed_by %></td>
        
        <td><%= link_to 'Show', cashfloat %></td>
        <td><%= link_to 'Edit', edit_cashfloat_path(cashfloat) %></td>
        <td><%= link_to 'Destroy', cashfloat, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cashfloat', new_cashfloat_path %>
