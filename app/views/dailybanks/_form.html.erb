<div class="col-md-10">
 <h6><small><b><%= @dailybank.effective_date.strftime('%A, %d %b %Y') %></b></small></h6> 

 
 <% case @dailybank.status %>
   <% when "Count Evening Till" %>
   <%= render 'dailybanks/cashfloat_new_pm' %>
        
 <% when "Enter Expenses"  %>
   <%= render 'dailybanks/expenses' %>
   
 <% when "Enter Cards"  %>
    <%= render 'dailybanks/cards_and_totals' %>   
       
 <% when "Enter Till Totals" %>
   <%= render 'dailybanks/till_totals' %>

 <% when "Validate and Lock" %>
   <%= render 'dailybanks/validate_lock' %>  
        
 <% when "Mgmt Review" %>
   <%= render 'dailybanks/finish_eon' %>
   
 <% when "Mgmt re-calc" %> 
  <%= render 'dailybanks/validate_lock' %>      
   
 <% when "Locked" %>
 <h6> This record has been Locked - if this was in error, click here to Unlock.
   
   </h6>
     
      
 <% else  %>
  <h6> Unknown status please contact IT depart with ref: DB_<%= @dailybank.id %>.</h6>
      
 <% end %>
 
 </div>
   
 
