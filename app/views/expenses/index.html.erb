<p id="notice"><%= notice %></p>

<h1>Listing Expenses</h1>

<table>
  <thead>
    <tr>
      <th>Dailybank</th>
      <th>What</th>
      <th>Where</th>
      <th>Price</th>
      <th>Price</th>
      <th>Ref</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @expenses.each do |expense| %>
      <tr>
        <td><%= expense.dailybank_id %></td>
        <td><%= expense.what %></td>
        <td><%= expense.where %></td>
        <td><%= expense.price %></td>
        <td><%= expense.price %></td>
        <td><%= expense.ref %></td>
        <td><%= link_to 'Show', expense %></td>
        <td><%= link_to 'Edit', edit_expense_path(expense) %></td>
        <td><%= link_to 'Destroy', expense, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Expense', new_expense_path %>
